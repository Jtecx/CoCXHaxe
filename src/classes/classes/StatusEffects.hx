    /**
		 * Creates status affect
		 */
    private static function mk(id : String, clazz : Class<Dynamic> = null, arity : Int = 1) : StatusEffectType
    {
        return new StatusEffectType(id, (clazz != null) ? clazz : StatusEffectClass, arity);
    }
    /**
		 * Creates combat status affect
		 */
    private static function mkCombat(id : String) : StatusEffectType
    {
        return new StatusEffectType(id, CombatStatusEffect, 1);
    }

